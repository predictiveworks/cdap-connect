{
  "metadata": {"spec-version": "1.5"},

  "display-name": "Ignite Sink",

  "icon": {
    "arguments": {
      "data": "data:image/png;base64,"
    },
    "type": "inline"
  },

  "configuration-groups": [
     {
      "label": "Plugin Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        }
 	    ]
    },
    {
      "label": "Endpoint Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Host",
          "name": "host"
        },
        {
          "widget-type": "textbox",
          "label": "Port",
          "name": "port"
        }
      ]
    },
    {
      "label": "Data Configuration",
      "properties": [
       {
          "widget-type": "textbox",
          "label": "Cache Name",
          "name": "cacheName"
       },
        {
          "widget-type": "select",
          "label": "Cache Mode",
          "name": "cacheMode",
          "widget-attributes": {
            "values": [
              "partitioned",
              "replicated"
            ],
            "default": "partitioned"
          }
        }
      ]
    },
    {
      "label": "Authentication",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Username",
          "name": "user"
        },
        {
          "widget-type": "password",
          "label": "Password",
          "name": "password"
        }
      ]
    },
    {
      "label": "SSL Security",
      "properties": [
         {
          "widget-type": "select",
          "label": "Use SSL",
          "name": "sslMode",
          "widget-attributes": {
            "values": [
              "false",
              "true"
            ],
            "default": "true"
          }
        },
        {
          "widget-type": "select",
          "label": "Verify Certificates",
          "name": "sslVerify",
          "widget-attributes": {
            "values": [
              "false",
              "true"
            ],
            "default": "true"
          }
        },
        {
          "widget-type": "csv",
          "label": "Cipher Suites",
          "name": "sslCipherSuites",
          "widget-attributes": {
            "delimiter": ","
          }
        },
        {
          "widget-type": "textbox",
          "label": "Keystore Path",
          "name": "sslKeyStorePath"
        },
        {
          "widget-type": "select",
          "label": "Keystore Type",
          "name": "sslKeyStoreType",
          "widget-attributes": {
            "values": [
              "JKS",
              "JCEKS",
              "PKCS12"
            ],
            "default": "JKS"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Keystore Algo",
          "name": "sslKeyStoreAlgo"
        },
        {
          "widget-type": "password",
          "label": "Keystore Pass",
          "name": "sslKeyStorePass"
        },
        {
          "widget-type": "textbox",
          "label": "Truststore Path",
          "name": "sslTrustStorePath"
        },
        {
          "widget-type": "select",
          "label": "Truststore Type",
          "name": "sslTrustStoreType",
          "widget-attributes": {
            "values": [
              "JKS",
              "JCEKS",
              "PKCS12"
            ],
            "default": "JKS"

          }
        },
        {
          "widget-type": "textbox",
          "label": "Truststore Algo",
          "name": "sslTrustStoreAlgo"
        },
        {
          "widget-type": "password",
          "label": "Truststore Pass",
          "name": "sslTrustStorePass"
        }
      ]
    }
  ]
}
