{
  "metadata": {
    "spec-version": "1.5"
  },

  "display-name": "Fiware Source",

  "icon": {
    "arguments": {
      "data": "data:image/png;base64,"
    },
    "type": "inline"
  },

  "configuration-groups": [
    {
      "label": "Plugin Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        }
      ]
    },
    {
      "label": "Endpoint Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Server Host",
          "name": "serverHost"
        },
        {
          "widget-type": "textbox",
          "label": "Server Port",
          "name": "serverPort"
        },
        {
          "widget-type": "textbox",
          "label": "Broker Url",
          "name": "brokerUrl"
        }

      ]
    },
    {
      "label": "Data Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Subscriptions",
          "name": "brokerSubscriptions"
        },
        {
          "widget-type": "textbox",
          "label": "Threads",
          "name": "numThreads",
          "widget-attributes": {
            "default": "1"
          }
        }
      ]
    },
    {
      "label": "SSL Server Security",
      "properties": [
        {
          "widget-type": "csv",
          "label": "Cipher Suites",
          "name": "serverSslCipherSuites",
          "widget-attributes": {
            "delimiter": ","
          }
        },
        {
          "widget-type": "textbox",
          "label": "Keystore Path",
          "name": "serverSslKeyStorePath"
        },
        {
          "widget-type": "select",
          "label": "Keystore Type",
          "name": "serverSslKeyStoreType",
          "widget-attributes": {
            "values": [
              "JKS",
              "JCEKS",
              "PKCS12"
            ],
            "default": "JKS"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Keystore Algo",
          "name": "serverSslKeyStoreAlgo"
        },
        {
          "widget-type": "password",
          "label": "Keystore Pass",
          "name": "serverSslKeyStorePass"
        },
        {
          "widget-type": "textbox",
          "label": "Truststore Path",
          "name": "serverSslTrustStorePath"
        },
        {
          "widget-type": "select",
          "label": "Truststore Type",
          "name": "serverSslTrustStoreType",
          "widget-attributes": {
            "values": [
              "JKS",
              "JCEKS",
              "PKCS12"
            ],
            "default": "JKS"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Truststore Algo",
          "name": "serverSslTrustStoreAlgo"
        },
        {
          "widget-type": "password",
          "label": "Truststore Pass",
          "name": "serverSslTrustStorePass"
        }
      ]
    },
    {
      "label": "SSL Broker Security",
      "properties": [
        {
          "widget-type": "csv",
          "label": "Cipher Suites",
          "name": "brokerSslCipherSuites",
          "widget-attributes": {
            "delimiter": ","
          }
        },
        {
          "widget-type": "textbox",
          "label": "Keystore Path",
          "name": "brokerSslKeyStorePath"
        },
        {
          "widget-type": "select",
          "label": "Keystore Type",
          "name": "brokerSslKeyStoreType",
          "widget-attributes": {
            "values": [
              "JKS",
              "JCEKS",
              "PKCS12"
            ],
            "default": "JKS"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Keystore Algo",
          "name": "brokerSslKeyStoreAlgo"
        },
        {
          "widget-type": "password",
          "label": "Keystore Pass",
          "name": "brokerSslKeyStorePass"
        },
        {
          "widget-type": "textbox",
          "label": "Truststore Path",
          "name": "brokerSslTrustStorePath"
        },
        {
          "widget-type": "select",
          "label": "Truststore Type",
          "name": "brokerSslTrustStoreType",
          "widget-attributes": {
            "values": [
              "JKS",
              "JCEKS",
              "PKCS12"
            ],
            "default": "JKS"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Truststore Algo",
          "name": "brokerSslTrustStoreAlgo"
        },
        {
          "widget-type": "password",
          "label": "Truststore Pass",
          "name": "brokerSslTrustStorePass"
        }
      ]
    }
  ]
}