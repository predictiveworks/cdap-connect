{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name": "OpenCTI Source",

  "icon": {
    "arguments": {
      "data": "data:image/png;base64,"
    },
    "type": "inline"
  },
  "configuration-groups": [
    {
      "label": "Plugin Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        }
      ]
    },
    {
      "label": "Endpoint Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Server URL",
          "name": "serverUrl"
        }
      ]
    },
    {
      "label": "Data Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Threads",
          "name": "numThreads",
          "widget-attributes": {
            "default": "1"
          }
        }
      ]
    },
    {
      "label": "Authentication",
      "properties": [
        {
          "widget-type": "password",
          "label": "Token",
          "name": "authToken"
        }
      ]
    },
    {
      "label": "SSL Security",
      "properties": [
        {
          "widget-type": "csv",
          "label": "Cipher Suites",
          "name": "sslCipherSuites",
          "widget-attributes": {
            "delimiter": ","
          }
        },
        {
          "widget-type": "textbox",
          "label": "Keystore Path",
          "name": "sslKeyStorePath"
        },
        {
          "widget-type": "select",
          "label": "Keystore Type",
          "name": "sslKeyStoreType",
          "widget-attributes": {
            "values": [
              "JKS",
              "JCEKS",
              "PKCS12"
            ],
            "default": "JKS"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Keystore Algo",
          "name": "sslKeyStoreAlgo"
        },
        {
          "widget-type": "password",
          "label": "Keystore Pass",
          "name": "sslKeyStorePass"
        },
        {
          "widget-type": "textbox",
          "label": "Truststore Path",
          "name": "sslTrustStorePath"
        },
        {
          "widget-type": "select",
          "label": "Truststore Type",
          "name": "sslTrustStoreType",
          "widget-attributes": {
            "values": [
              "JKS",
              "JCEKS",
              "PKCS12"
            ],
            "default": "JKS"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Truststore Algo",
          "name": "sslTrustStoreAlgo"
        },
        {
          "widget-type": "password",
          "label": "Truststore Pass",
          "name": "sslTrustStorePass"
        }
      ]
    }
  ]
}
