{
  "metadata": {"spec-version": "1.5"},

  "display-name": "Things Http Sink",

  "icon": {
    "arguments": {
      "data": "data:image/png;base64,"
    },
    "type": "inline"
  },
  "configuration-groups": [
    {
      "label": "Plugin Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        }
 	  ]
    },
    {
      "label": "Endpoint Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Host",
          "name": "host"
        },
        {
          "widget-type": "textbox",
          "label": "Port",
          "name": "port"
        }
      ]
    },
    {
      "label": "Data Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Asset Name",
          "name": "assetName"
        },
        {
          "widget-type": "textbox",
          "label": "Asset Type",
          "name": "assetType"
        },
        {
          "widget-type": "textbox",
          "label": "Asset Limit",
          "name": "assetLimit",
          "widget-attributes": {
            "default": "1000"
          }
        },
        {
          "widget-type": "csv",
          "label": "Asset Features",
          "name": "assetFeatures",
          "widget-attributes": {
            "delimiter": ","
          }
        }
      ]
    },
    {
      "label": "Authentication",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Username",
          "name": "user"
        },
        {
          "widget-type": "password",
          "label": "Password",
          "name": "password"
        }
      ]
    }
  ]
}