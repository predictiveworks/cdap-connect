{
  "metadata": {"spec-version": "1.5"},

  "display-name": "Crate Source",

  "icon": {
    "arguments": {
      "data": "data:image/png;base64,..."
    },
    "type": "inline"
  },

  "configuration-groups": [
    {
      "label": "Plugin Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        }
 	  ]
    },
    {
      "label": "Endpoint Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Host",
          "name": "host"
        },
        {
          "widget-type": "textbox",
          "label": "Port",
          "name": "port"
        }
      ]
    },
    {
      "label": "Data Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Table Name",
          "name": "tableName"
        },
        {
          "widget-type": "textarea",
          "label": "Insert Query",
          "name": "inputQuery",
          "widget-attributes": {
            "rows": "10"
          }
        }
      ]
    },
    {
      "label": "Authentication",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "User Name",
          "name": "user"
        },
        {
          "widget-type": "textbox",
          "label": "Password",
          "name": "password"
        }
      ]
    }
  ]
}