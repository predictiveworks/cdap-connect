{
  "metadata": {"spec-version": "1.5"},

  "display-name": "Snowflake Source",

  "icon": {
    "arguments": {
      "data": "data:image/png;base64,"
    },
    "type": "inline"
  },

  "configuration-groups": [
    {
      "label": "Plugin Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        }
 	  ]
    },
    {
      "label": "Endpoint Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Host",
          "name": "host"
        },
        {
          "widget-type": "textbox",
          "label": "Port",
          "name": "port"
        },
        {
          "widget-type": "textbox",
          "label": "Account",
          "name": "account"
        },
        {
          "widget-type": "select",
          "label": "Region",
          "name": "region",
          "widget-attributes": {
            "values": [
              "us-west-2",
              "us-east-1",
              "ca-central-1",
              "eu-west-1",
              "eu-central-1",
              "ap-southeast-1",
              "ap-southeast-2",
              "east-us-2.azure",
              "canada-central.azure",
              "west-europe.azure",
              "australia-east.azure",
              "southeast-asia.azure"
            ],
            "default": "us-west-2"
          }
        }
      ]
    },
    {
      "label": "Data Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Warehouse Name",
          "name": "warehouse"
        },
        {
          "widget-type": "textbox",
          "label": "Database Name",
          "name": "database"
        },
        {
          "widget-type": "textbox",
          "label": "Database Schema",
          "name": "schema"
        },
        {
          "widget-type": "textbox",
          "label": "Table Name",
          "name": "tableName"
        },
        {
          "widget-type": "textarea",
          "label": "Import Query",
          "name": "inputQuery",
          "widget-attributes": {
            "rows": "10"
          }
        }
 	    ]
    },
    {
      "label": "Authentication",
      "properties": [
        {
          "widget-type": "select",
          "label": "SSL",
          "name": "ssl",
          "widget-attributes": {
            "values": [
              "on",
              "off"
            ],
            "default": "on"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Username",
          "name": "user"
        },
        {
          "widget-type": "textbox",
          "label": "User Role",
          "name": "role"
        },
        {
          "widget-type": "select",
          "label": "Authenticator",
          "name": "authenticator",
          "widget-attributes": {
            "values": [
              "snowflake",
              "externalbrowser",
              "oauth",
              "snowflake_jwt",
              "username_password_mfa"
            ],
            "default": "snowflake"
          }
        },
        {
          "widget-type": "textbox",
          "label": "OAuth Token",
          "name": "authToken"
        },
        {
          "widget-type": "password",
          "label": "Password",
          "name": "password"
        }
      ]
    }
  ]
}
