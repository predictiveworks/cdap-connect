{
  "metadata": {
    "spec-version": "1.5"
  },

  "display-name": "HiveMQ Source",

  "icon": {
    "arguments": {
      "data": "data:image/png;base64,"
    },
    "type": "inline"
  },

  "configuration-groups": [
    {
      "label": "Plugin Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        }
      ]
    },
    {
      "label": "Endpoint Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Broker Host",
          "name": "mqttHost"
        },
        {
          "widget-type": "textbox",
          "label": "Broker Port",
          "name": "mqttPort"
        }
      ]
    },
    {
      "label": "Data Configuration",
      "properties": [
        {
          "widget-type": "csv",
          "label": "Topics",
          "name": "mqttTopics",
          "widget-attributes": {
            "delimiter": ","
          }
        },
        {
          "widget-type": "select",
          "label": "QoS",
          "name": "mqttQoS",
          "widget-attributes": {
            "values": [
              "at-most-once",
              "at-least-once",
              "exactly-once"
            ],
            "default": "at-most-once"
          }
        },
        {
          "widget-type": "select",
          "label": "Version",
          "name": "mqttVersion",
          "widget-attributes": {
            "values": [
              "mqtt-v311",
              "mqtt-v5"
            ],
            "default": "mqtt-v311"
          }
        }
      ]
    },
    {
      "label": "Authentication",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Username",
          "name": "mqttUser"
        },
        {
          "widget-type": "textbox",
          "label": "Password",
          "name": "mqttPass"
        }
      ]
    },
    {
      "label": "Security",
      "properties": [
        {
          "widget-type": "csv",
          "label": "Cipher Suites",
          "name": "sslCipherSuites",
          "widget-attributes": {
            "delimiter": ","
          }
        },
        {
          "widget-type": "textbox",
          "label": "Keystore Path",
          "name": "sslKeyStorePath"
        },
        {
          "widget-type": "select",
          "label": "Keystore Type",
          "name": "sslKeyStoreType",
          "widget-attributes": {
            "values": [
              "JKS",
              "JCEKS",
              "PKCS12"
            ],
            "default": "JKS"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Keystore Algo",
          "name": "sslKeyStoreAlgo"
        },
        {
          "widget-type": "textbox",
          "label": "Keystore Pass",
          "name": "sslKeyStorePass"
        },
        {
          "widget-type": "textbox",
          "label": "Truststore Path",
          "name": "sslTrustStorePath"
        },
        {
          "widget-type": "select",
          "label": "Truststore Type",
          "name": "sslTrustStoreType",
          "widget-attributes": {
            "values": [
              "JKS",
              "JCEKS",
              "PKCS12"
            ],
            "default": "JKS"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Truststore Algo",
          "name": "sslTrustStoreAlgo"
        },
        {
          "widget-type": "textbox",
          "label": "Truststore Pass",
          "name": "sslTrustStorePass"
        }
      ]
    }
  ]

}