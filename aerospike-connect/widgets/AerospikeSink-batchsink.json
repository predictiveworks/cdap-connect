{
  "metadata": {"spec-version": "1.5"},

  "display-name": "Aerospike Sink",

  "icon": {
    "arguments": {
      "data": "data:image/png;base64,..."
    },
    "type": "inline"
  },

  "configuration-groups": [
    {
      "label": "Plugin Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        }
 	  ]
    },

    {
      "label": "Endpoint Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Host",
          "name": "host"
        },
        {
          "widget-type": "textbox",
          "label": "Port",
          "name": "port"
        },
        {
          "widget-type": "textbox",
          "label": "Timeout",
          "name": "timeout",
          "widget-attributes": {
            "default": "1000"
          }
        }
      ]
    },
    {
      "label": "Data Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Namespace",
          "name": "namespace"
        },
        {
          "widget-type": "textbox",
          "label": "Set Name",
          "name": "setname"
        },
        {
          "widget-type": "textbox",
          "label": "Expiration",
          "name": "expiration",
          "widget-attributes": {
            "default": "0"
          }
        },
        {
          "widget-type": "select",
          "label": "Write Option",
          "name": "writeOption",
          "widget-attributes": {
            "values": [
              "Append",
              "ErrorIfExists",
              "Ignore",
              "Overwrite"
            ],
            "default": "Append"
          }
        }
      ]
    },
    {
      "label": "Authentication",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Username",
          "name": "user"
        },
        {
          "widget-type": "textbox",
          "label": "Password",
          "name": "password"
        }
      ]
    }
  ]
}
