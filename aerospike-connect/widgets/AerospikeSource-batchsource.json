{
  "metadata": {"spec-version": "1.5"},

  "display-name": "Aerospike Source",

  "icon": {
    "arguments": {
      "data": "data:image/png;base64,"
    },
    "type": "inline"
  },
  "configuration-groups": [
    {
      "label": "Plugin Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        }
 	  ]
    },
    {
      "label": "Endpoint Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Host",
          "name": "host"
        },
        {
          "widget-type": "textbox",
          "label": "Port",
          "name": "port"
        },
        {
          "widget-type": "textbox",
          "label": "Timeout",
          "name": "timeout",
          "widget-attributes": {
            "default": "1000"
          }
        }
      ]
    },
    {
      "label": "Data Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Namespace",
          "name": "namespace"
        },
        {
          "widget-type": "textbox",
          "label": "Set Name",
          "name": "setname"
        }
      ]
    },
    {
      "label": "Authentication",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Username",
          "name": "user"
        },
        {
          "widget-type": "password",
          "label": "Password",
          "name": "password"
        }
      ]
    }
  ]
}
